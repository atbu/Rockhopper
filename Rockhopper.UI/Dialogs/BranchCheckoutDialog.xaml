<Window x:Class="Rockhopper.UI.Dialogs.BranchCheckoutDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:Rockhopper.UI"
        mc:Ignorable="d"
        Title="BranchCheckoutDialog" Height="150" Width="250">
    <Grid>
        <StackPanel>
            <TextBlock Text="Branch to checkout:"/>
            <TextBox x:Name="BranchTextBox" />
            <Button Content="OK" Click="OKButton_Click" />
        </StackPanel>
    </Grid>
</Window>
